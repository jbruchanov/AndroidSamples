<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="@color/td_primaryButtonBackgroundRipple">
    <item>
        <selector>
            <item android:state_enabled="false">
                <shape>
                    <solid android:color="@color/td_primaryButtonBackgroundDisabled"/>
                    <corners android:radius="@dimen/td_buttonRadius"/>
                </shape>
            </item>
            <item>
                <shape>
                    <solid android:color="@color/td_primaryButtonBackgroundDefault"/>
                    <corners android:radius="@dimen/td_buttonRadius"/>
                </shape>
            </item>
        </selector>
    </item>
</ripple>