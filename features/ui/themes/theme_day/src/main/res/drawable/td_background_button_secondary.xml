<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/td_secondaryButtonBackgroundDisabled"/>
            <corners android:radius="@dimen/td_buttonRadius"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/td_secondaryButtonBackgroundPressed"/>
            <corners android:radius="@dimen/td_buttonRadius"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/td_secondaryButtonBackgroundDefault"/>
            <corners android:radius="@dimen/td_buttonRadius"/>
        </shape>
    </item>
</selector>